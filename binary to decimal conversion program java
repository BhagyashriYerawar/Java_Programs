public class Main {
	public static void main(String[] args) {
		int num=10101001;
		int result=binToDec(num);
		System.out.println(result);
		
	}
	public static int  binToDec(int num) {
		int dec_val=0;
		int temp=num;
		int base=1;
		
        while(temp>0) {
        	int last_digit=temp%10;
        	dec_val=dec_val+base*last_digit;
        	temp=temp/10;
        	base=base*2;
        	
        	
        	
        }
        return dec_val;
	}

}
